<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0"
	prefWidth="1300.0" stylesheets="@../stylesheet.css"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.ryhma6.maven.steambeater.controller.SteamOpenIDSignController">
	<top>
		<HBox nodeOrientation="LEFT_TO_RIGHT" prefHeight="32.0"
			prefWidth="1300.0" spacing="5.0">
			<children>
				<Button fx:id="loginButton" mnemonicParsing="false"
					onAction="#handleLoginButtonAction">
					<HBox.margin>
						<Insets left="2.0" right="58.0" top="11.0" />
					</HBox.margin>
				</Button>
				<Button fx:id="refreshButton" maxHeight="27.0"
					onAction="#refreshData">
					<HBox.margin>
						<Insets right="10.0" top="8.0" />
					</HBox.margin>
				</Button>
				<Button fx:id="signTestButton" maxHeight="27.0"
					onAction="#loadWithTestValues" text="test sign">
					<HBox.margin>
						<Insets right="10.0" top="8.0" />
					</HBox.margin>
				</Button>
				<Button fx:id="logoutButton" maxHeight="30.0"
					onAction="#logout" stylesheets="@../stylesheet.css" text="logout">
					<HBox.margin>
						<Insets top="8.0" />
					</HBox.margin>
				</Button>
				<Label fx:id="loadStateLabel" text="">
					<HBox.margin>
						<Insets left="25.0" top="6.0" />
					</HBox.margin>
				</Label>
				<HBox fx:id="taskbarHbox" alignment="TOP_RIGHT"
					maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT"
					prefHeight="52.0" HBox.hgrow="ALWAYS">
					<children>
						<Button fx:id="btnClose" mnemonicParsing="false"
							onMouseClicked="#handleMouseEvent" />
						<Button fx:id="btnFull" mnemonicParsing="false"
							onMouseClicked="#handleMouseEvent" />
						<Button fx:id="btnMinimize" mnemonicParsing="false"
							onMouseClicked="#handleMouseEvent" />
						<Pane maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
					</children>
					<opaqueInsets>
						<Insets />
					</opaqueInsets>
				</HBox>
			</children>
			<BorderPane.margin>
				<Insets bottom="30.0" top="15.0" />
			</BorderPane.margin>
		</HBox>
	</top>
	<left>
		<FlowPane fx:id="sidebar" prefHeight="200.0" prefWidth="20.0"
			BorderPane.alignment="CENTER" />
	</left>
	<padding>
		<Insets bottom="20.0" left="20.0" right="20.0" />
	</padding>
</BorderPane>
